{
  "name": "bizley/mercure-behavior",
  "description": "Yii 2 Mercure behavior.",
  "type": "yii2-extension",
  "keywords": ["yii2", "mercure", "broadcast", "behavior"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    }
  ],
  "support": {
    "source": "https://github.com/bizley/mercure-behavior"
  },
  "require": {
    "php": ">=7.2.0",
    "bizley/yii2-mercure": "^1.0",
    "yiisoft/yii2": ">=2.0.13 <2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "bizley\\yii2\\behaviors\\mercure\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bizley\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
